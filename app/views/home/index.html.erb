<h1>Home#index</h1>
<p>Find me in app/views/home/index.html.erb</p>
 
 <!-- <%=debug(@tag_result)%>  -->
 <!-- shows the debug mssg of our variable tag result -->

 <% @tag_result['data'].each do |result| %>
  		<% if result['type'] == ['video'] %>
  			

  			
  			<video width = "320 height = "240" controls>
  			<source src="<%= result['videos']['standard_resolution']['url'] %>" >
  			</video>

  		<% else %>
  			<img src= "<%= result['images']['standard_resolution']['url'] %>">
  		<% end %>
  	<% end %>